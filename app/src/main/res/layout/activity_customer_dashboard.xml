<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bglight">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:drawableLeft="@drawable/ic_baseline_arrow_black_24"
        android:drawablePadding="50dp"
        android:gravity="center"
        android:text="@string/cus_dash"
        android:textColor="@color/black"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.151"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView3"
        android:layout_width="84dp"
        android:layout_height="77dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/profile_picture_view"
        android:src="@drawable/user"
        app:layout_constraintBottom_toTopOf="@+id/btnaddaddress"
        app:layout_constraintEnd_toStartOf="@+id/btnup"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:srcCompat="@drawable/user" />

    <Button
        android:id="@+id/btnup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:backgroundTint="@color/btn_orange"
        android:text="@string/update_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/button5"
        android:layout_width="197dp"
        android:layout_height="47dp"
        android:layout_marginBottom="122dp"
        android:backgroundTint="@color/btn_orange"
        android:text="@string/comp_orders"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView24"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toEndOf="@+id/imageView29"
        app:layout_constraintTop_toBottomOf="@+id/imageView23"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="TouchTargetSizeCheck,TextContrastCheck" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="193dp"
        android:layout_height="170dp"
        android:layout_marginStart="70dp"
        android:layout_marginEnd="84dp"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.593"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnviewaddress"
        app:srcCompat="@drawable/completed" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="68dp"
        android:layout_height="106dp"
        android:layout_marginStart="324dp"
        android:layout_marginTop="238dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnviewfeedback"
        app:srcCompat="@drawable/money" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="68dp"
        android:layout_height="106dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="224dp"
        android:layout_marginBottom="42dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView24"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/amount" />

    <Button
        android:id="@+id/btnaddaddress"
        android:layout_width="148dp"
        android:layout_height="45dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="30dp"
        android:backgroundTint="@color/btn_orange"
        android:text="Add Address"
        app:layout_constraintBottom_toTopOf="@+id/btnviewaddress"
        app:layout_constraintEnd_toStartOf="@+id/btnaddfeedback"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <Button
        android:id="@+id/btnviewaddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="48dp"
        android:backgroundTint="@color/btn_orange"
        android:text="View Address"
        app:layout_constraintBottom_toTopOf="@+id/imageView23"
        app:layout_constraintEnd_toStartOf="@+id/btnviewfeedback"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnaddaddress" />

    <Button
        android:id="@+id/btnaddfeedback"
        android:layout_width="176dp"
        android:layout_height="45dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="29dp"
        android:backgroundTint="@color/btn_orange"
        android:text="Add Feedback"
        app:layout_constraintBottom_toTopOf="@+id/btnviewfeedback"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnaddaddress"
        app:layout_constraintTop_toBottomOf="@+id/btnup" />

    <Button
        android:id="@+id/btnviewfeedback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="@color/btn_orange"
        android:text="View Feedback"
        app:layout_constraintBottom_toTopOf="@+id/imageView23"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnviewaddress"
        app:layout_constraintTop_toBottomOf="@+id/btnaddfeedback" />

</androidx.constraintlayout.widget.ConstraintLayout>